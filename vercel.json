{
  "version": 2,
  "name": "test",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/test.js": {
      "maxDuration": 30
    },
    "api/*.js": {
      "maxDuration": 15
    }
  }
}
